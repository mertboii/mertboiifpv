                                <a class="text-decoration-none nav-link link-d ml-3 my-1" href="#item-1-1" style="color:dimgray">Frames</a>
                                <a class="text-decoration-none nav-link link-d ml-3 my-1" href="#item-1-1" style="color:dimgray">Propellers</a>


                            </nav>

                        </nav>
                    </nav>


                </div>

                <div class="col-md-10">

                    @RenderBody()

                </div>

            </div>



        </main>
