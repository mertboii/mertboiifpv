                                <a class="text-decoration-none nav-link link-d ml-3 my-1" asp-controller="CetusDrones" asp-action="Index" style="color:dimgray">Cetus Series</a>
                                <a class="text-decoration-none nav-link ml-3 my-1" asp-controller="MeteorDrones" asp-action="Index" style="color:dimgray">Meteor Series</a>
